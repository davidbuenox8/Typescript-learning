nal":{"line":2,"column":0},"generated":{"line":82,"column":1}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":0},"generated":{"line":82,"column":3}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":0}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":7}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":8}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":16}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":19}},{"source":"src/views/UserForm.ts","name":null,"original":{"line":2,"column":13},"generated":{"line":84,"column":27}}],"sources":{"src/views/UserForm.ts":"import { User } from '../models/User';\r\nexport class UserForm {\r\n  constructor(public parent: Element, public model: User) {\r\n    this.bindModel();\r\n  }\r\n\r\n  bindModel(): void {\r\n    this.model.on('change', () => {\r\n      this.render();\r\n    });\r\n  }\r\n  eventsMap(): { [key: string]: () => void } {\r\n    return {\r\n      'click:.set-age': this.onSetAgeClick,\r\n      'click:.set-name': this.onSetNameClick,\r\n    };\r\n  }\r\n  onSetAgeClick = (): void => {\r\n    this.model.setRandomAge();\r\n  };\r\n  onSetNameClick = (): void => {\r\n    const input = this.parent.querySelector('input');\r\n    if (input) {\r\n      const name = input.value;\r\n      this.model.set({ name });\r\n    }\r\n  };\r\n  template(): string {\r\n    return `\r\n    <div>\r\n      <h1>User Form</h1>\r\n      <div>User Name: ${this.model.get('name')}</div>\r\n      <div>User Age: ${this.model.get('age')}</div>\r\n      <input/>\r\n      <button class='set-name'>Change Me</button>\r\n      <button class='set-age'>Set Random Age</button>\r\n    </div>\r\n      `;\r\n  }\r\n  bindEvents(fragment: DocumentFragment): void {\r\n    const eventsMap = this.eventsMap();\r\n    for (let eventKey in eventsMap) {\r\n      const [eventName, selector] = eventKey.split(':');\r\n      fragment.querySelectorAll(selector).forEach((element) => {\r\n        element.addEventListener(eventName, eventsMap[eventKey]);\r\n      });\r\n    }\r\n  }\r\n  render(): void {\r\n    this.parent.innerHTML = '';\r\n    const templateElement = document.createElement('template');\r\n    templateElement.innerHTML = this.template();\r\n    this.bindEvents(templateElement.content);\r\n    this.parent.append(templateElement.content);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"99fd4efe6d5c1e6d437a1ee5b03f432d","cacheData":{"env":{}}}